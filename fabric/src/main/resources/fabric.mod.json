{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://github.com/cotrin8672",
    "sources": "https://github.com/cotrin8672/ItemIndicator"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "value": "ItemIndicatorFabricClient",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=21",
    "architectury": ">=${architectury_version}",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
